+int * golfread(const int arraysize)
+{
+	int * scores = new int[arraysize];
+	
+	cout << "Enter golf scores to be crunched\n#1: ";
+
+	for (int loop = 0; loop < arraysize && cin >> scores[loop]; ++loop)
+		cout << "#" << loop+1 << ": ";
+
+	return scores;
+}
