+void ch7_ex1(void)
+{
+	double xval, yval;
+	
+	cout << "Enter two values to be evaluated to harmony: ";
+	while ((cin >> xval >> yval))
+	{
+		if (xval == 0 && yval == 0)
+			break;
+
+		double mean = harmony(xval, yval);
+
+		cout << "Harmonic Mean: " << mean << endl;
+		cout << "Bring harmony to the digits: ";
+	}
+}
